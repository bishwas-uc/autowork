clean_name=`echo $1 | sed 's/\://' | sed 's/\-//g'`
basic_page=$2_$clean_name
party_page=$basic_page'_party'
docket_page=$basic_page'_docket'
/usr/local/bin/aws s3 cp s3://codaxtr_html/pacer/data/$basic_page.html /home/codaxtr_user/s3bucket/pacer/data/
/usr/local/bin/aws s3 cp s3://codaxtr_html/pacer/data/$party_page.html /home/codaxtr_user/s3bucket/pacer/data/
/usr/local/bin/aws s3 cp s3://codaxtr_html/pacer/data/$docket_page.html /home/codaxtr_user/s3bucket/pacer/data/

